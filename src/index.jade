extends templates/wrapper
block vars 
    //- Load data about users
    include data/boardData

block title
    | YUDI - Homepage

block content
    //- Navbar
    include templates/layouts/nav
    
    include templates/partials/header
    //- Announcements Section
    include templates/partials/calendar
    include templates/partials/about
    include templates/partials/team
    include templates/partials/clients
    include templates/partials/donate
    include templates/partials/contact
    include templates/layouts/footer
